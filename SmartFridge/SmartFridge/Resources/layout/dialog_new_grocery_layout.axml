<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="500px"
    android:layout_height="700px"
    android:minWidth="500px"
    android:minHeight="700px"
	android:weightSum="100">
	<TextView
		android:text="Dodaj novu namirnicu"
		android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_width="match_parent"
		android:gravity="center"
		android:layout_margin="10dp"
		android:layout_height="wrap_content"
		android:id="@+id/textView1" />
	<android.support.v7.widget.Toolbar
		 android:minHeight="?android:attr/actionBarSize"
        android:background="@color/material_grey_100"
        android:layout_width="match_parent"
        android:layout_height="?android:attr/actionBarSize"
		android:layout_gravity="center"
        android:id="@+id/toolbarTopGroceriesList">
		<LinearLayout
			android:orientation="horizontal"
			android:layout_width="match_parent"
			android:focusableInTouchMode="true"
			android:layout_height="match_parent"
			android:id="@+id/linearLayout3">
			<Syncfusion.Android.ComboBox.SfComboBox
				android:layout_width="match_parent"
				android:gravity="center"
				android:layout_height="match_parent"
				android:id="@+id/cmboBoxCategories" />
		</LinearLayout>
		</android.support.v7.widget.Toolbar>
	<Com.Syncfusion.Autocomplete.SfAutoComplete
		android:layout_width="match_parent"
		android:layout_margin="10dp"
		android:layout_height="wrap_content"
		android:id="@+id/autoCompleteGrocery" />
	<EditText
		android:layout_width="match_parent"
		android:hint="Kolicina"
		android:inputType="numberDecimal"
		android:layout_height="wrap_content"
		android:id="@+id/editTxtAmountGrocery" />
	<TextView
		android:text="Unit"
		android:textAppearance="?android:attr/textAppearanceLarge"
		android:layout_width="match_parent"
		android:gravity="left"
		android:layout_margin="2dp"
		android:layout_height="wrap_content"
		android:id="@+id/txtUnitNewGroceryDialog" />
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:weightSum="100"
		android:layout_height="wrap_content">
		<Button
			android:text="OK"
			android:layout_weight="50"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/btnOK"
			android:layout_marginTop="10dp" />
		<Button
			android:text="OtkaÅ¾i"
			android:layout_weight="50"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="10dp"
			android:id="@+id/btnCancel" />
	</LinearLayout>
</LinearLayout>